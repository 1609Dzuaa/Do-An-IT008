<ui:UiPage x:Class="MotoStore.Views.Pages.IOPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MotoStore.Views.Pages"
      xmlns:models="clr-namespace:MotoStore.Models"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      d:DataContext="{d:DesignInstance local:IOPage,
                                     IsDesignTimeCreatable=False}"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Scrollable="True"
      Title="IOPage">

    <Grid>
        <ui:Button HorizontalAlignment="Center" VerticalAlignment="Center" Command="{Binding ViewModel.OpenDialogCommand}" Content="Open" />
    </Grid>
</ui:UiPage>
